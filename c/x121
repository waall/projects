#include <stdio.h>
#include <stdlib.h>

int main (void){
  int A[5][5],len=5;
    for(int i=0;i<len;i++)
      for(int j=0;j<len;j++){
        A[i][j]=rand()%100;
        if(i==j){//DIAGONAL PRINCIPAL
          printf("%d ",A[i][j]);}
    }
  printf("\n\n");
    for(int i=0;i<len;i++){
      for(int j=0;j<len;j++){
        printf("%d ",A[i][j]);}
    printf("\n");}
printf("\x0a");
	return 0;}
